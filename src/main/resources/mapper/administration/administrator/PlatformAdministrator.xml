<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xwwwww.purchaseplatform.mapper.administration.administrator.PlatformAdministratorMapper">
    <select id="login" parameterType="com.xwwwww.purchaseplatform.entity.administration.administrator.PlatformAdministrator"
            resultType="com.xwwwww.purchaseplatform.entity.administration.administrator.PlatformAdministrator">
        select * from platform_administrator where user_Name=#{userName} and password=#{password}
    </select>
</mapper>
