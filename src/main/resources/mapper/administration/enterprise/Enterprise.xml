<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xwwwww.purchaseplatform.mapper.administration.enterprise.EnterpriseMapper">
    <select id="getAllEnterprise" parameterType="String" resultType="com.xwwwww.purchaseplatform.entity.administration.enterprise.Enterprise">
        select * from enterprise
    </select>
    <select id="deleteEnterpriseById"  resultType="com.xwwwww.purchaseplatform.entity.administration.enterprise.Enterprise">
        delete from enterprise where id=${id}
    </select>
</mapper>
